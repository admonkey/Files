{
    "name": "kenarkose/files",
    "description": "Flexible file and media management for Laravel 5",
    "license": "MIT",
    "keywords": [
        "kenarkose",
        "files",
        "media",
        "management",
        "laravel",
        "laravel5"
    ],
    "version": "0.9.0",
    "authors": [
        {
            "name": "Hyleeh",
            "email": "egemen@kenarkose.com",
            "homepage": "http://egemenkizilcan.com",
            "role": "Developer / Designer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "laravel/framework": "5.0.*"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Kenarkose\\Files\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ]
    },
    "suggest": {
        "kenarkose/transit": "Easy file upload and download for Laravel 5.",
        "kenarkose/ownable": "Allows ownership for Eloquent models on Laravel 5."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
